<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0"
	prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.ProblemsList">
	<children>
		<HBox layoutX="578.0" prefHeight="58.0" prefWidth="200.0"
			spacing="10.0">
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
		</HBox>
		<VBox layoutX="27.0" layoutY="29.0" prefHeight="533.0"
			prefWidth="746.0" spacing="8.0">
			<children>
				<Text strokeType="OUTSIDE" strokeWidth="0.0"
					text="Task Description :">
					<font>
						<Font size="18.0" />
					</font>
				</Text>
				<TextArea fx:id="taskDescription" prefHeight="110.0"
					prefWidth="746.0" />
				<Text strokeType="OUTSIDE" strokeWidth="0.0"
					text="Your Solution :">
					<font>
						<Font size="18.0" />
					</font>
				</Text>
				<TextArea fx:id="studentSolution" editable="false"
					layoutX="10.0" layoutY="34.0" prefHeight="110.0" prefWidth="746.0" />
				<HBox prefHeight="25.0" prefWidth="746.0">
					<children>
						<Text strokeType="OUTSIDE" strokeWidth="0.0"
							text="Your Grade :">
							<font>
								<Font size="18.0" />
							</font>
						</Text>
						<Label prefHeight="36.0" prefWidth="354.0">
							<font>
								<Font size="18.0" />
							</font>
						</Label>
					</children>
				</HBox>
				<Text layoutX="10.0" layoutY="164.0" strokeType="OUTSIDE"
					strokeWidth="0.0" text="Feedback : ">
					<font>
						<Font size="18.0" />
					</font>
				</Text>
				<TextArea fx:id="tutorFeedback" editable="false"
					layoutX="10.0" layoutY="37.0" prefHeight="154.0" prefWidth="746.0" />
			</children>
		</VBox>
		<HBox layoutX="552.0" layoutY="555.0" prefHeight="30.0"
			prefWidth="221.0" spacing="10.0">
			<children>
				<Button fx:id="prev" mnemonicParsing="false"
					onAction="#prevButton" prefHeight="30.0" prefWidth="85.0"
					text="Previous" />
				<Button fx:id="next" mnemonicParsing="false"
					onAction="#nextButton" prefHeight="30.0" prefWidth="85.0"
					text="Next" />
			</children>
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
		</HBox>
		<Button fx:id="back" layoutX="27.0" layoutY="562.0"
			minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
			onAction="#backToMyAcc" prefHeight="30.0" prefWidth="181.0"
			text="Back to My Account" />
		<Text layoutX="331.0" layoutY="21.0" strokeType="OUTSIDE"
			strokeWidth="0.0" text="Your solved tasks" textAlignment="CENTER"
			underline="true">
			<font>
				<Font size="18.0" />
			</font>
		</Text>
	</children>
</AnchorPane>
